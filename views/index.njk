{% extends "layout.njk" %}
{% block content %}
<main>


      


    {% for row in rows %}
    <section>
        {{ row.id }} {{ row.authorId }} {{ row.author }}
        <h1>{{ row.title }}</h1>
        <p>{{ row.content }}</p>
        <p>Created {{ row.createdAt }}</p>
        <p>{{ row.name }}</p>
    </section>
    {% endfor %}

</main>
{% endblock %}